pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Build step'
                sleep 10
            }
        }
          stage('Test step') {
            steps {
                echo 'Test step'
            }
        }
          stage('Deploy') {
            steps {
                echo 'Deploy step'
                sleep 10
            }
        }
          stage('Docker') {
            steps {
                echo 'image step'
            }
        }
    }
} 
